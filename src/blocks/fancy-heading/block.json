{
    "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
    "apiVersion": 2,
    "name": "self-development/fancy-header",
    "title": "Fancy Header",
    "category": "test",
    "icon": "star-full",
    "description": "Adds a header with a hover effect",
    "keywords": ["header", "underline", "hover"],
    "version": "1",
    "textdomain": "self-development",
    "editorScript": "file:./index.js",
    "attributes": {
        "content": {
            "type": "string",
            "source": "html",
            "selector": "h2"
        },
        "underlineColor": {
            "type": "string",
            "default": "#f87171"
        }
    },
    "style": "file:./index.css"
}